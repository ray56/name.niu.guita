            name="Generate Test Case Offline">
      </command>
      <command
            id="name.niu.guitar.ui.commands.GenOnline"
            name="Generate Test Case Online">
         <commandParameter
               id="name.niu.guitar.ui.commands.GenOnline.subCmd"
               name="subCmd"
